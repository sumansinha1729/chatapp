{
  "expo": {
    "name": "ChatApp",
    "slug": "chatapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#007AFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chatapp"
    },
    "android": {
      "package": "com.chatapp",
      "adaptiveIcon": {
        "backgroundColor": "#007AFF"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
